@using WebApplication1.Models.FormDataMappe;
@model IEnumerable<WebApplication1.Models.FormDataMappe.FormData>


@{
    ViewData["Title"] = "ServiceForm";
}

<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<link rel="stylesheet" href="~/css/serviceform.css" asp-append-version="true" />
<script src="~/js/serviceform.js" asp-append-version="true"></script>

@foreach (var brukere in Model)
{
        <br />
        <td class="table">kundeID: @brukere.id</td>
        <br />
        <td class="table">Navn: @brukere.Navn</td>
        <br />
        <td class="table">Telefonnummer: @brukere.TelefonNummer</td>
        <br />
        <td class="table">Kommentar: @brukere.Kommentar  </td>
        <br />
}

